{
    "name": "LLVM",
    "id": "llvm",
    "version": "1.0.8",
    "description": "Installs LLVM using the automatic installation script",
    "options": {
        "version": {
            "type": "string",
            "enum": [
                "9",
                "10",
                "11",
                "12",
                "13",
                "14",
                "15",
                "16",
                "17",
                "latest"
            ],
            "default": "latest",
            "description": "Select a specific LLVM version or use latest (default)"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}